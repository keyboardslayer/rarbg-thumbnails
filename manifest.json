{
  "manifest_version": 2,
  "name": "RARBG thumbnails",
  "short_name": "RARBG thumbs",
  "description": "Display thumbnails automaticly on RARBG",
  "version": "1.1.3",
  "content_scripts":
  [
    {
      "matches":
      [
        "*://rarbg.to/top*",
        "*://rarbg.to/torrents.php*",
        "*://rarbg.is/top*",
        "*://rarbg.is/torrents.php*",
        "*://rarbgproxy.org/top*",
        "*://rarbgproxy.org/torrents.php*",
        "*://rarbgmirror.xyz/top*",
        "*://rarbgmirror.xyz/torrents.php*",
        "*://rarbgunblocked.org/top*",
        "*://rarbgunblocked.org/torrents.php*"
      ],
      "js":
      [
        "content.js"
      ],
      "css":
      [
        "content.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons":
  {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
